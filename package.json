  "license": "MIT",
  "dependencies": {
    "js-to-html": "~1.0.4"
  },
  "devDependencies": {
    "electron": "^5.0.0",
    "electron-builder": "^20.39.0"
  },
  "build": {
    "appId": "com.electron.git-tool",
    "mac": {
      "category": "guid git tool"
    }
  }
